{
    "name": "laravel/market-analysis-platform",
    "type": "project",
    "description": "Enterprise Market Analysis Platform with Real-time Data, Quantitative Analysis, and LLM Integration",
    "keywords": ["laravel", "market-analysis", "crypto", "forex", "stocks", "quantitative", "llm", "gemini"],
    "license": "MIT",
    "require": {
        "php": "^8.3|^8.4",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "predis/predis": "^2.2",
        "guzzlehttp/guzzle": "^7.8",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-data": "^4.7",
        "spatie/laravel-queueable-action": "^2.14",
        "spatie/laravel-ray": "^1.37",
        "laravel/telescope": "^5.2",
        "laravel/horizon": "^5.28",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-csp": "^2.9",
        "spatie/laravel-backup": "^9.0",
        "doctrine/dbal": "^4.0",
        "lcobucci/jwt": "^5.0",
        "ext-json": "*",
        "ext-bcmath": "*",
        "ext-gmp": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.38",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.5",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.8",
        "laravel/dusk": "^8.2",
        "pestphp/pest": "^3.0",
        "larastan/larastan": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Domain/Shared/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}